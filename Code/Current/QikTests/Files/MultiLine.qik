
@codeInput =>
		"{" + NEWLINE +
			indentLine("int x = y;", TAB, 1) + NEWLINE +
			indentLine("if (x == 2)", TAB, 1) + NEWLINE +
			indentLine("return y;", TAB, 2) + NEWLINE +
			indentLine("else", TAB, 1) + NEWLINE +
			indentLine("return x;", TAB, 2) + NEWLINE +
		"}\n";

@displayEvents =>
	"events: {" + NEWLINE +
		doubleQuotes("hello world") + NEWLINE +
	"};";