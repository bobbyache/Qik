{
    "name": "qikt",
    "displayName": "qikt",
    "description": "Qik Template",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.58.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "qiktemplate",
            "aliases": ["Qik Template Language", "qiktemplate"],
            "extensions": [".qikt", ".qiktemplate"],
            "configuration": "./language-configuration.json"
        },
        {
            "id": "qikscript",
            "aliases": ["Qik Script Language", "qikscript"],
            "extensions": [".qik", ".qikscript"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "qiktemplate",
            "scopeName": "source.qiktemplate",
            "path": "./syntaxes/qiktemplate.tmLanguage.json"
        },{
            "language": "qikscript",
            "scopeName": "source.qikscript",
            "path": "./syntaxes/qikscript.tmLanguage.json"
        }]
    }
}