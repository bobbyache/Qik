{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Qik Template Language",
	"patterns": [
		{ "include": "#keywords" }
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.qiktemplate",
				"match": "@{[a-zA-Z0-9_]*}"
			}]
		}
	},
	"scopeName": "source.qiktemplate"
}